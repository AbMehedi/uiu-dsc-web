{
  "name": "uiu-dsc-clone",
  "version": "1.0.0",
  "description": "UIU Data Science Club Website Clone - Full-stack application with Node.js, TypeScript, Express, and SQLite",
  "main": "dist/server.js",
  "scripts": {
    "build": "tsc",
    "vercel-build": "tsc",
    "start": "node dist/server.js",
    "dev": "tsc && nodemon dist/server.js",
    "dev:watch": "nodemon --watch src --ext ts --exec \"npm run build && node dist/server.js\"",
    "clean": "rmdir /s /q dist 2>nul || echo Cleaned",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "data-science",
    "club",
    "uiu",
    "express",
    "typescript",
    "sqlite"
  ],
  "author": "UIU Data Science Club",
  "license": "ISC",
  "type": "commonjs",
  "dependencies": {
    "@types/express-session": "^1.18.2",
    "ejs": "^3.1.10",
    "express": "^5.1.0",
    "express-session": "^1.18.2",
    "multer": "^2.0.2",
    "sqlite3": "^5.1.7"
  },
  "devDependencies": {
    "@types/express": "^5.0.5",
    "@types/multer": "^2.0.0",
    "@types/node": "^24.10.1",
    "@types/sqlite3": "^3.1.11",
    "nodemon": "^3.1.11",
    "typescript": "^5.9.3"
  }
}
